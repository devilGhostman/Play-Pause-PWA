{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/Movie/Cards/TreadingCards/TreandingCard.tsx", "components/Movie/Cards/TreadingCards/Tcard.tsx", "components/Music/MainContainer/MainContainer.jsx", "components/Music/Radio/Radio.jsx", "pages/api/auth/[...nextauth].jsx", "pages/watchtogether/[seriesid]/index.tsx", "pages/watchtogether.tsx"],
  "exclude": ["node_modules"]
}
